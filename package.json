{
  "name": "norwegian-rental",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@phosphor-icons/react": "^2.1.7",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.0.0",
    "react-datepicker": "^8.2.1",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.6.0",
    "react-scripts": "5.0.1",
    "react-time-picker": "^7.0.0",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "concurrently \"npm run frontend\" \"npm run backend\"",
    "frontend": "cross-env PORT=3001 react-scripts start",
    "backend": "mvn spring-boot:run",
    "wipedatabase": "cross-env SPRING_JPA_HIBERNATE_DDL_AUTO=create-drop mvn spring-boot:run",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint-config-react-app": "^7.0.1",
    "node-fetch": "^2.7.0",
    "node-polyfill-webpack-plugin": "^4.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/src/resources/__mocks__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
